        <script><![CDATA[function onChange(control, oldValue, newValue, isLoading) {
   if (isLoading)
      return;

   if (newValue == '') {
      g_form.setValue('u_contact_phone', '');
      return;
   }

   if (!g_form.getControl('u_contact_phone'))
      return;

   var caller = g_form.getReference('u_key_contact', setLocation);
}

function setLocation(caller) {
   if (caller)
  
       g_form.setValue('u_contact_phone', caller.u_contact_phone);


}]]></script>
